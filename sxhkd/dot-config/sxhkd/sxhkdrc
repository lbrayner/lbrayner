Print
    screenshot

super + Print
    snip

super + ctrl + shift + l
    slock

super + w
    rofi -show window

super + r
    rofi -show drun -show-icons

super + y
    x11primary2clip

# mpc, mpv

super + comma
    mpc pause; \
    pkill -45 dwmblocks; \
    echo '\{ "command": ["set_property", "pause", true] \}' | socat - /tmp/mpvsocket

super + period
    mpc toggle; \
    pkill -45 dwmblocks; \
    echo '\{ "command": ["set_property", "pause", true] \}' | socat - /tmp/mpvsocket

super + semicolon
    mpc pause; \
    pkill -45 dwmblocks; \
    echo "cycle pause" | socat - /tmp/mpvsocket

# vim:ft=conf
